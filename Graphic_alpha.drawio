        <mxCell value="&lt;div style=&quot;text-align: left&quot;&gt;&lt;div&gt;&lt;div&gt;static int msm_drm_init(struct device *dev, struct drm_driver *drv)&lt;/div&gt;&lt;div&gt;{&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; struct drm_device *ddev = drm_dev_alloc(drv, dev);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; drm_mode_config_init(ddev);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; //call kms-&amp;gt;funcs to enable or disable vblank&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; kthread_init_work(&amp;amp;priv-&amp;gt;vblank_ctrl.work, vblank_ctrl_worker);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = sde_power_resource_init(pdev, &amp;amp;priv-&amp;gt;phandle);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; priv-&amp;gt;pclient = sde_power_client_create(&amp;amp;priv-&amp;gt;phandle, &quot;sde&quot;);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; /* Bind all our sub-components: call component&#39;s bind()*/&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = msm_component_bind_all(dev, ddev);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // allocate struct sde_kms, and set &lt;b&gt;sde_kms.msm_kms.funcs&lt;/b&gt;;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; struct msm_kms *kms = sde_kms_init(ddev);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; priv-&amp;gt;kms = kms;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // parse DTS, init hw&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // create work &quot;delayed_off_work&quot; and &quot;vsync_event_work&quot; and&amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // run in disp_thread and event_thread&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // abstract out and init drm_encode/drm_bridge/drm_connetor/&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // drm_plane/drm_crtc components with properties&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = kms-&amp;gt;funcs-&amp;gt;hw_init(kms);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ddev-&amp;gt;mode_config.funcs = &amp;amp;&lt;b&gt;mode_config_funcs&lt;/b&gt;;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // create event_thread(&quot;crtc_event:%d&quot;) and&amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // disp_thread(&quot;crtc_commit:%d&quot;) for each drm_crtc&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; for (i = 0; i &amp;lt; priv-&amp;gt;num_crtcs; i++) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; /* initialize display thread */&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; kthread_init_worker(&amp;amp;priv-&amp;gt;disp_thread[i].worker);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; priv-&amp;gt;disp_thread[i].thread = kthread_run(kthread_worker_fn,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;amp;priv-&amp;gt;disp_thread[i].worker,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&quot;crtc_commit:%d&quot;, priv-&amp;gt;disp_thread[i].crtc_id);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; /* initialize event thread */&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; kthread_init_worker(&amp;amp;priv-&amp;gt;event_thread[i].worker);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; priv-&amp;gt;event_thread[i].thread = kthread_run(kthread_worker_fn,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;amp;priv-&amp;gt;event_thread[i].worker,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &quot;crtc_event:%d&quot;, priv-&amp;gt;event_thread[i].crtc_id);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // setup timer, initialize vblank support&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = drm_vblank_init(ddev, priv-&amp;gt;num_crtcs);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // request irq with drv-&amp;gt;irq_handler&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = drm_irq_install(ddev, platform_get_irq(pdev, 0));&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // register drm_device to DRM framework&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = drm_dev_register(ddev, 0);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; //create debug files in /sys/kernel/debug/dri/0/debug/&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; priv-&amp;gt;debug_root = debugfs_create_dir(&quot;debug&quot;,&amp;nbsp;&lt;span&gt;ddev-&amp;gt;primary-&amp;gt;debugfs_root);&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = sde_dbg_debugfs_register(priv-&amp;gt;debug_root);&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; /* perform subdriver post initialization */&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // create /sys/class/drm/sde_crtc_%d/vsync_event&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; ret = kms-&amp;gt;funcs-&amp;gt;postinit(kms);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // create work for connetor states check, it is used for&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; // some device&lt;span&gt;&amp;nbsp;which not have hotplug&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; drm_kms_helper_poll_init(ddev);&lt;/div&gt;&lt;div&gt;}&lt;br&gt;&lt;/div&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;/div&gt;" id="0fYE4SzcJHIdMOUISsD_-34" parent="0fYE4SzcJHIdMOUISsD_-33" style="html=1;align=left;" vertex="1">
