        <mxCell value="&lt;div style=&quot;text-align: left&quot;&gt;&lt;div&gt;static struct platform_driver &lt;b&gt;msm_platform_driver &lt;/b&gt;= {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &lt;b&gt;.probe&amp;nbsp; &amp;nbsp; &amp;nbsp; = msm_pdev_probe,&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; .remove&amp;nbsp; &amp;nbsp; &amp;nbsp;= msm_pdev_remove,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; .shutdown&amp;nbsp; &amp;nbsp;= msm_pdev_shutdown,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp;.driver&amp;nbsp; &amp;nbsp; &amp;nbsp;= {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;.name&amp;nbsp; &amp;nbsp;= &quot;msm_drm&quot;,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;b&gt;.of_match_table = dt_match,&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;.pm&amp;nbsp; &amp;nbsp; &amp;nbsp;= &amp;amp;msm_pm_ops,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; },&lt;/div&gt;&lt;div&gt;};&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;static int __init &lt;b&gt;msm_drm_register&lt;/b&gt;(void)&lt;/div&gt;&lt;div&gt;{&lt;/div&gt;&lt;div&gt;&lt;span&gt;&amp;nbsp; &amp;nbsp; // register msm_smmu_driver platform driver,&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&amp;nbsp; &amp;nbsp; // match DTS Node:&lt;/span&gt;&lt;span&gt;&amp;nbsp;&quot;qcom,smmu_sde_sec&quot;,&amp;nbsp;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;span&gt;&amp;nbsp; &amp;nbsp; //&amp;nbsp; create mapping to mdp_s domain2G-4G&lt;/span&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; msm_smmu_driver_init();&amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; msm_dsi_register(); // KCONFIG not set, it is null&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; msm_edp_register(); // KCONFIG not set, it is null&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; msm_hdmi_register(); // KCONFIG not set, it is null&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; adreno_register(); // KCONFIG not set, it is null&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; return &lt;b&gt;platform_driver_register(&amp;amp;msm_platform_driver);&lt;/b&gt;&lt;/div&gt;&lt;div&gt;}&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;b&gt;module_init(msm_drm_register);&lt;/b&gt;&lt;/div&gt;&lt;div&gt;module_exit(msm_drm_unregister);&lt;/div&gt;&lt;/div&gt;" id="0fYE4SzcJHIdMOUISsD_-16" parent="0fYE4SzcJHIdMOUISsD_-15" style="html=1;align=left;" vertex="1">
